{
  "name": "synthetix-mintr",
  "version": "2.21.12",
  "private": true,
  "license": "MIT",
  "author": "Synthetix",
  "dependencies": {
    "@reduxjs/toolkit": "^1.2.5",
    "@rehooks/component-size": "^1.0.3",
    "browserslist": "^4.8.2",
    "date-fns": "^2.0.1",
    "ethers": "4.0.38",
    "i18next": "^17.0.4",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "keccak": "^2.0.0",
    "lodash": "^4.17.15",
    "numbro": "^2.1.2",
    "rc-slider": "^8.7.1",
    "rc-tooltip": "4.0.0-alpha.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-calendar": "^2.19.2",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.9",
    "react-i18next": "^10.11.2",
    "react-outside-click-handler": "^1.2.4",
    "react-redux": "^7.2.0",
    "react-responsive-carousel": "^3.1.51",
    "react-select": "^3.0.4",
    "react-switch": "^5.0.1",
    "styled-components": "^4.3.2",
    "styled-system": "^5.1.0",
    "synthetix-data": "^2.1.11",
    "synthetix-js": "2.21.12",
    "walletlink": "^1.0.0",
    "snyk": "^1.316.1"
  },
  "scripts": {
    "clean-install": "rm ./package-lock.json && rm -rf node_modules/* && npm i",
    "start": "node --max-old-space-size=4096 scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "i18n-extract": "cp nodes",
    "lint": "eslint .",
    "lint:css": "stylelint src/**/**/*.js",
    "format": "prettier --write '**/*.js'",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.4.3",
    "@hot-loader/react-dom": "^16.9.0",
    "@svgr/webpack": "4.1.0",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-react-app": "^9.0.0",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "css-loader": "2.1.1",
    "dotenv": "^6.2.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.7.1",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.3.0",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "papaparse": "^5.0.2",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.11",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "semver": "6.0.0",
    "style-loader": "0.23.1",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "ts-pnp": "1.1.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.2.0"
  },
  "snyk": true
}
